<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="%JAVA_HOME%\bin\java.exe"
        arguments="-Djava.net.preferIPv4Stack=true -Dspring.profiles.active=azure -jar '%HOME%\site\wwwroot\web-0.0.1-SNAPSHOT.jar'">
    </httpPlatform>
  </system.webServer>
</configuration>